    "framer-motion": "^11.2.10",
    "immer": "^10.1.1",
    "lucide-react": "^0.394.0",
    "next-themes": "^0.3.0",
    "numbro": "^2.5.0",
    "plasmo": "^0.87.2",
    "rc-drawer": "^7.2.0",
